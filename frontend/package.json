{
  "name": "inos-frontend",
  "private": true,
  "version": "1.8.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node ../scripts/fix-capnp-imports.js && vite build",
    "preview": "vite preview",
    "proto": "capnp compile -I../protocols/schemas -o ts:./bridge/generated ../protocols/schemas/**/*.capnp && node ../scripts/fix-capnp-imports.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "prettier": "prettier check \"**/*{js,jsx,json,ts,tsx,yaml}\"",
    "prettier-fix": "prettier --write \"**/*{js,jsx,json,ts,tsx,yaml}\"",
    "format": "yarn lint --fix && yarn prettier-fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^8.17.10",
    "@react-three/postprocessing": "2.16.2",
    "@vercel/analytics": "^1.6.1",
    "d3": "^7.9.0",
    "framer-motion": "^12.24.12",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.12.0",
    "react-use": "^17.6.0",
    "styled-components": "^6.2.0",
    "three": "^0.171.0",
    "three-stdlib": "^2.36.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/styled-components": "^5.1.36",
    "@types/three": "^0.171.0",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "@vitejs/plugin-basic-ssl": "^2.1.3",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^4.0.16",
    "capnp-es": "^0.0.11",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "vite": "^6.0.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-wasm": "^3.3.0",
    "vitest": "^4.0.16"
  },
  "resolutions": {
    "three-mesh-bvh": "^0.8.0"
  }
}
